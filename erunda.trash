int i = 0, j, tmp, n;
int b[100];
scan(n);  // читаем количество элементов массива (5)
while (i < n) {
    scan(b[i]);
    i = i + 1;
}

while (i < n) {
    j = 0;
    while (j < n - 1) {
        if (b[j] > b[j + 1]) {
            tmp = b[j];
            b[j] = b[j + 1];
            b[j + 1] = tmp;
        }
        j = j + 1;
    }
    i = i + 1;
}

i = 0;
while (i < n) {
    print(b[i]);
    i = i + 1;
}